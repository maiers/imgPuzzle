              <Properties>
                <Property name="mnemonic" type="int" value="110"/>
                <Property name="text" type="java.lang.String" value="Number of Tiles"/>
              </Properties>
              <AuxValues>
                <AuxValue name="JavaCodeGenerator_LayoutCodePre" type="java.lang.String" value="for (int i = 3; i &lt; 10; i++) {&#xd;&#xa;    for (int j = 0; j &lt; 2; j++) {&#xd;&#xa;        JRadioButtonMenuItem btn = new JRadioButtonMenuItem(setTileCount);&#xd;&#xa;        tileButtonGroup.add(btn);&#xd;&#xa;        btn.setText(&quot;&quot; + i * (i+j));&#xd;&#xa;        btn.setActionCommand(i + &quot;,&quot; + (i+j));&#xd;&#xa;        btn.setSelected(imagePuzzlePanel1.getModel().getNumberOfTiles() == i * (i+j));&#xd;&#xa;        tileMenu.add(btn);&#xd;&#xa;    }&#xd;&#xa;}"/>
              </AuxValues>
            </Menu>
            <Menu class="javax.swing.JMenu" name="durationMenu">
              <Properties>
                <Property name="mnemonic" type="int" value="97"/>
                <Property name="text" type="java.lang.String" value="Answer duration"/>
              </Properties>
              <AuxValues>
                <AuxValue name="JavaCodeGenerator_LayoutCodePre" type="java.lang.String" value="        for (int i = 1; i &lt;= 10; i++) {&#xd;&#xa;            JRadioButtonMenuItem btn = new JRadioButtonMenuItem(setAnswerDuration);&#xd;&#xa;            answerDurationButtonGroup.add(btn);&#xd;&#xa;            btn.setText(String.format(&quot;%d seconds&quot;, i));&#xd;&#xa;            btn.setActionCommand(&quot;&quot; + i*1000);&#xd;&#xa;            btn.setSelected(imagePuzzlePanel1.getModel().getAnswerDuration() == i * 1000);&#xd;&#xa;            durationMenu.add(btn);&#xd;&#xa;        }"/>
              </AuxValues>
            </Menu>
